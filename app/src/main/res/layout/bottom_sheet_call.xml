<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/transparent_color"
    android:orientation="vertical"
    android:paddingBottom="25dp"
    tools:context=".presentation.profile.CallFragmentDialog">

    <TextView
        android:id="@+id/contact_phone_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="8dp"
        android:layout_marginVertical="18dp"
        android:background="@drawable/bg_call"
        android:fontFamily="@font/sf_text_regular"
        android:gravity="center"
        android:padding="16dp"
        android:text="8 705 665 00 01"
        android:textColor="@color/color_dark_1000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/cansel_contact_phone_number"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="8dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/bg_call"
        android:fontFamily="@font/sf_display_semibold"
        android:gravity="center"
        android:padding="16dp"
        android:text="Отмена"
        android:textColor="@color/color_blue_1000"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/contact_phone_number" />

</androidx.constraintlayout.widget.ConstraintLayout>